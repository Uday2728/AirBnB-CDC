{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "Air-BnB-Data-Factory"
		},
		"cosmos_Air_BnB_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'cosmos_Air_BnB'"
		},
		"ADLSForAirBnB_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://airbnbdata.dfs.core.windows.net/"
		},
		"AzureSynapseForAirBnB_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "mysynapsewokspacedemo.sql.azuresynapse.net"
		},
		"AzureSynapseForAirBnB_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "Air_BnB_WH"
		},
		"StoredProcedureforFactTbl_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "mysynapsewokspacedemo.sql.azuresynapse.net"
		},
		"StoredProcedureforFactTbl_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "Air_BnB_WH"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "Air-BnB-Data-Factory_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"cosmos_Air_BnB_connectionString": {
						"value": "[parameters('cosmos_Air_BnB_connectionString')]"
					},
					"ADLSForAirBnB_properties_typeProperties_url": {
						"value": "[parameters('ADLSForAirBnB_properties_typeProperties_url')]"
					},
					"AzureSynapseForAirBnB_properties_typeProperties_server": {
						"value": "[parameters('AzureSynapseForAirBnB_properties_typeProperties_server')]"
					},
					"AzureSynapseForAirBnB_properties_typeProperties_database": {
						"value": "[parameters('AzureSynapseForAirBnB_properties_typeProperties_database')]"
					},
					"StoredProcedureforFactTbl_properties_typeProperties_server": {
						"value": "[parameters('StoredProcedureforFactTbl_properties_typeProperties_server')]"
					},
					"StoredProcedureforFactTbl_properties_typeProperties_database": {
						"value": "[parameters('StoredProcedureforFactTbl_properties_typeProperties_database')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}